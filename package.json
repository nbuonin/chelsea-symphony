{
  "name": "chelsea-symphony",
  "version": "0.1.0",
  "description": "Website of the Chelsea Symphony",
  "license": "GPL-2.0+",
  "scripts": {
    "test": "mocha",
    "build-scss": "node-sass --output chelseasymphony/static/css chelseasymphony/static/scss/main.scss --output-style compressed --source-map true",
    "watch-scss": "nodemon --watch chelseasymphony/static/scss --verbose -e scss -x 'npm run build-scss'"
  },
  "devDependencies": {
    "eslint": "~5.0.0",
    "eslint-plugin-no-unsafe-innerhtml": "~1.0.16",
    "eslint-plugin-scanjs-rules": "~0.2.1",
    "eslint-plugin-security": "~1.4.0",
    "mocha": "~5.2.0",
    "requirejs": "~2.3.2",
    "sinon": "~6.0.0",
    "autoprefixer": "~9.3.0",
    "node-sass": "~4.9.0",
    "nodemon": "~1.18.0",
    "postcss-cli": "~6.0.0"
  }
}
